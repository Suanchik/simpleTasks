const penal = (state = initial, action) => {
  switch (action.type) {
    case 'ADD_CARD':
      debugger;
      return { ...state,
        items: { ...state.items,
          [action.el]: { ...state.items[action.el],
            item: [...state.items[action.el].item, {
              name: action.text
            }]
          }
        }
      };

    case 'ADD_NEW_LIST':
      return { ...state,
        items: { ...state.items,
          [action.newName]: action.obj
        }
      };

    case 'DELETE_ALL_CARD':
      {
        const newItems = { ...state.items
        };
        delete newItems[action.elementName];
        return { ...state,
          items: newItems
        };
      }

    case 'ADD_NEW_NAME':
      {
        const newItems = { ...state.items
        };
        newItems[action.el].name = action.newName;
        return { ...state,
          items: newItems
        };
      }

    default:
      return state;
  }
};

const addNewCardItem = (el, text) => {
  return {
    type: "ADD_CARD",
    el,
    text
  };
};
const addNewListItem = (obj, newName) => {
  return {
    type: "ADD_NEW_LIST",
    obj,
    newName
  };
};
